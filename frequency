#include<stdio.h>
#include<string.h>
int main()
{
    char a[20]="muskan",ch;
    int count=0;
    printf("enter a character to find its frequency:");
    scanf("%c",&ch);
    for (int i=0;str[i]!='\0';++i)
    {
        if(ch==str[i])
        ++count;
    }
    
    printf("frequencyt of %c=%d",ch, count);
    return 0;
}
    
